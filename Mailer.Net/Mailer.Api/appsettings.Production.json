{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AllowOrigins": "#{allowOrigins}",
  "ConnectionStrings": {
    "Mailer": "#{connectionString}"
  },
  "SmtpHostAddress": "#{hostAddress}",
  "MailRetryIntervalMilliseconds": 5000,
  "SmtpLogin": "#{smtpLogin}",
  "SmtpPassword": "#{smtpPassword}"
}